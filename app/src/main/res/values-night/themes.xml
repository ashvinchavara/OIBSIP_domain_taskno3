<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Base.Theme.Calc" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/blue</item>
        <item name="colorPrimaryVariant">@color/blue</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/ivory</item>
        <item name="colorSecondaryVariant">@color/light_ivory</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
        <item name="black">@color/black</item>
        <item name="white">@color/white</item>
        <item name="darkblue">@color/darkblue</item>
        <item name="blue" >@color/blue</item>
        <item name="ivory" >@color/ivory</item>
        <item name="stand">@color/stand</item>
        <item name="dis" >@color/dis</item>
        <item name="calc">@color/calc</item>

    </style>
    <style name="Theme.Calc" parent="Base.Theme.Calc" />
</resources>
